bool isGreater(int num, int test) {
    return num > test;
}

void main(void) {
    int i;
    int a[5];
    int test;
    int count;

    test = 5;
    count = 0;
    i = 0;
    while(i < 5) {
        a[i] = input();
        output(i);
        i = i + 1;
    }

    i = 0;
    while(i < 5) {
        if (a[i] > test) {
            count = count + 1;
        }
        i = i + 1;
    }

    output(count);
}
