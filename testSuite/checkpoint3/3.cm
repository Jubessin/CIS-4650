/*
*   File Name: 3.cm
*   Description: Tests that the CM compiler correctly generates assembly code for the recursive version of fibonacci.
*/

int fibonacci(int n) {
    if (n <= 0)
        return 0;
    else if (n == 1)
        return 1;
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main(void) {
    int num;
    
    /* enter a number */
    num = input();
    
    if (num < 0) {
        return 0;
    }
    else {
        output(fibonacci(num));
    }
    
    return 0;
}
