int _integer;
bool _boolean;

bool _isPositive(int value) 
{
    return value > 0;
}

void main(void) {
    _integer = 77;

    while (_integer < 80) {
        _integer = _integer + 1;
    }

    if (_isPositive(2)) {
        _boolean = true;
    }
    else {
        _boolean = false;
    }

    return;
}
